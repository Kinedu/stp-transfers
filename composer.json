{
    "name": "kinedu/stp-transfers",
    "type": "project",
    "description": "Module for accepting STP electronic transfers.",
    "keywords": [
        "laravel",
        "kinedu",
        "billing",
        "electronic transfers",
        "stp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Bentinck",
            "email": "ed@kinedu.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Kinedu\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
